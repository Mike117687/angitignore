{
    "name": "AnGitIgnored",
    "displayName": "An GitIgnored",
    "description": "Tool that facilitates the management of .ignore files",
    "license": "MIT",
    "preview": true,
    "icon": "images/logoAnGitIgnoredV000003.png",
    "version": "1.1.0",
    "publisher": "AnAppWiLos",
    "engines": {
        "vscode": "^1.19.0"
    },
    "keywords": [
        "git",
        "gitHub",
        ".gitignore",
        "vs"
    ],
    "categories": [
        "Git"
    ],
    "activationEvents": [
        "*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/anappwilos/gitignore.git"
    },
    "homepage": "https://github.com/anappwilos/gitignore.git",
    "bugs": {
        "url": "https://github.com/anappwilos/gitignore.git"
    },
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.addGitignored",
                "title": "Add Gitignored : ‚ûïAdd to .gitignore"
            },
            {
                "command": "extension.hideGitignored",
                "title": "Hide Gitignored: üëªHide files ignored by .gitignore"
            },
            {
                "command": "extension.showGitignored",
                "title": "Show Gitignored: üëÅ‚Äçüó®Show files ignored by .gitignore"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.addGitignored",
                    "group": "navigation"
                },
                {
                    "command": "extension.hideGitignored",
                    "group": "navigation"
                },
                {
                    "command": "extension.showGitignored",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.48",
        "@types/node": "^7.0.66",
        "typescript": "^2.9.2",
        "vscode": "^1.1.18"
    }
}
